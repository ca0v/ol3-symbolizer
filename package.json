{
  "name": "ol3-symbolizer",
  "version": "5.1.3",
  "description": "Deserialize ol3 styles",
  "main": "built/index.js",
  "scripts": {
    "clean": "rmdir .\\built /s/q",
    "update": "npm install ol3-fun reverse-proxy-cache",
    "doc": "typedoc --out ./built/docs --tsconfig ./tsconfig.json --readme none",
    "build:amd": "tsc -p tsconfig.json",
    "build:loaders": "tsc -p loaders/tsconfig.json",
    "build:tests": "esbuild ./tests/index.ts --outfile=./built/loaders/test.js --watch --watch --format=esm --target=esnext",
    "lint:tests": "tsc -p tests/tsconfig.json --watch",
    "watch:tests": "npm-run-all -p build:tests lint:tests",
    "build:examples": "tsc -p examples/tsconfig.json --outFile ./built/examples.max.js",
    "live:tests": "esbuild ./tests/index.ts --outfile=./loaders/tests.js --watch --bundle --format=esm --target=esnext",
    "live:loader": "esbuild ./loaders/tests.ts --outfile=./loaders/loader.js --servedir=./loaders --bundle --format=esm --target=esnext",
    "dev:tests": "npm-run-all -p gateway live:tests live:loader",
    "live:examples": "live-server --port=5001 --wait=200 --watch=./built/*.max.js --open=\"./loaders/tests.html?theme=dark\" --browser=CHROME",
    "dev:examples": "npm run build:examples & start npm run live:examples & npm run build:examples -- -w",
    "uglify:amd": "cd built && uglifyjs index.max.js --source-map \"content='index.max.js.map',url='index.js.map'\" --output=index.js --compress --mangle",
    "tests": "npm run dev:tests",
    "move": "mv built/index.max.d.ts built/index.d.ts",
    "before-push": "npm-run-all clean build:loaders build:amd build:tests build:examples move test",
    "install": "npm run build:amd",
    "gateway": "reverse-proxy-cache",
    "test": "npm run dev:tests"
  },
  "pre-commit": [
    "before-push"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca0v/ol3-symbolizer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ca0v/ol3-symbolizer/issues"
  },
  "homepage": "https://github.com/ca0v/ol3-symbolizer#readme",
  "dependencies": {
    "@ca0v/ceylon": "file:../ceylon",
    "@types/jquery": "latest",
    "@types/ol": "latest",
    "esbuild": "^0.13.15",
    "husky": "^7.0.4",
    "jquery": "latest",
    "live-server": "latest",
    "mocha": "latest",
    "npm-run-all": "latest",
    "ol": "^6.9.0",
    "ol3-fun": "file:../ol3-fun",
    "requirejs": "^2.3.6",
    "reverse-proxy-cache": "file:../reverse-proxy-cache",
    "typescript": "^4.5.2"
  },
  "reverse-proxy-cache": {
    "port": "3001",
    "reverse-proxy-db": "unittest.sqlite",
    "proxy-pass": [
      {
        "about": "requests to /mock redirect to https://usalvwdgis1.infor.com:6443",
        "baseUri": "/mock/ags",
        "proxyUri": "https://usalvwdgis1.infor.com:6443/arcgis"
      },
      {
        "baseUri": "/mock/geoserver",
        "proxyUri": "http://localhost:8080/geoserver"
      }
    ]
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0"
  }
}
